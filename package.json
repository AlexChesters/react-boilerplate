{
  "name": "react-redux-boilerplate",
  "version": "0.0.1",
  "description": "",
  "engines": {
    "node": "8.9.1",
    "npm": "5.5.1"
  },
  "scripts": {
    "build:dev": "webpack --config webpack.dev.config.js",
    "build:prod": "webpack --config webpack.prod.config.js",
    "start": "webpack-dev-server --config webpack.dev.config.js --port 8081",
    "lint:styles": "stylelint \"src/**/*.scss\" --syntax scss --config .stylelintrc.json",
    "lint:scripts": "eslint --config .eslintrc.json --ext js --ext jsx src test",
    "lint": "concurrently \"npm run lint:styles\" \"npm run lint:scripts\"",
    "unit": "jest --config .jestrc.json",
    "test": "concurrently \"npm run unit\" \"npm run lint\""
  },
  "pre-commit": [
    "lint"
  ],
  "pre-push": [
    "unit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexChesters/react-redux-bootstrap-boilerplate.git"
